file = {:D$\backslash$:/Mendeley/2018/2018 - Thermal diffusivity of ferrofluids as a function of particle size determined using the mode-mismatched dual-beam thermal lens tec.pdf:pdf},
issn = {0021-8979},
journal = {Journal of Applied Physics},
month = {feb},
number = {8},
pages = {085107},
title = {{Thermal diffusivity of ferrofluids as a function of particle size determined using the mode-mismatched dual-beam thermal lens technique}},
url = {http://aip.scitation.org/doi/10.1063/1.5017025},
volume = {123},
year = {2018}
